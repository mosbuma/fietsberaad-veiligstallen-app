    "maplibre-gl": "^2.0.0-pre.1",
    "mime": "^3.0.0",
    "moment-timezone": "^0.5.45",
    "next": "^15.3.2",
    "next-auth": "^4.23.1",
    "next-pwa": "^5.6.0",
    "next-redux-wrapper": "^8.1.0",
    "nodemailer": "^6.9.12",
    "react": "18.2.0",
    "react-apexcharts": "^1.5.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "^2.4.1",
