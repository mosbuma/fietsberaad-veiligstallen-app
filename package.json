{
  "name": "veiligstallen-app",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0 <19.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "postinstall": "prisma generate",
    "lint": "eslint .",
    "start": "next start",
    "setup-db-local": "cd mysql-db && bash setup-db.sh && cd .. && prisma db pull --force && prisma generate",
    "start-db-local": "docker start veiligstallen-mysql",
    "start-db": "cd scripts && bash launch-ssh-tunnel.sh && echo 'SSH tunnel started'",
    "stop-db-local": "docker stop veiligstallen-mysql",
    "stop-db": "cd scripts && bash close-ssh-tunnel.sh && echo 'SSH tunnel stopped'",
    "remove-db": "docker stop veiligstallen-mysql && docker rm veiligstallen-mysql",
    "deploy": "cd scripts && bash deploy.sh",
    "docker-build": "bash scripts/build-docker-local.sh",
    "docker-run": "docker rm -f veiligstallen-app && docker run -p 3000:3000 --network=host --name veiligstallen-app nextjs-docker"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.5.0",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@fontsource/roboto": "^4.5.8",
    "@lexical/code": "^0.31.0",
    "@lexical/link": "^0.31.0",
    "@lexical/list": "^0.31.0",
    "@lexical/markdown": "^0.31.0",
    "@lexical/react": "^0.31.0",
    "@lexical/rich-text": "^0.31.0",
    "@lexical/table": "^0.31.0",
    "@mapbox/mapbox-gl-draw": "^1.3.0",
    "@mapbox/mapbox-gl-draw-static-mode": "^1.0.1",
    "@mui/material": "^5.12.0",
    "@premieroctet/next-crud": "^2.3.2",
    "@reduxjs/toolkit": "^1.9.5",
    "@tanstack/react-query": "^4.28.0",
    "@trpc/client": "^10.18.0",
    "@trpc/next": "^10.18.0",
    "@trpc/react-query": "^10.18.0",
    "@trpc/server": "^10.18.0",
    "@turf/center": "^6.5.0",
    "@turf/turf": "^6.5.0",
    "@types/moment-timezone": "^0.5.30",
    "apexcharts": "^4.0.0",
    "babel-plugin-superjson-next": "^0.4.5",
    "bcryptjs": "^2.4.3",
